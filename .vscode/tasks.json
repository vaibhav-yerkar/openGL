{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: clang build active file",
      "command": "/usr/bin/clang++",
      "args": [
        "-fcolor-diagnostics",
        "-fansi-escape-codes",
        "-g",
        "${file}",
        "${fileDirname}/Header/display.cpp",
        "${fileDirname}/Header/mesh.cpp",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}",
        "-I/opt/homebrew/Cellar/sdl2/2.30.9/include/SDL2",
        "-L/opt/homebrew/Cellar/sdl2/2.30.9/lib",
        "-lSDL2",
        "-I/opt/homebrew/Cellar/glew/2.2.0_1/include/GL",
        "-L/opt/homebrew/Cellar/glew/2.2.0_1/lib",
        "-lGLEW",
        "-I/opt/homebrew/Cellar/glm/1.0.1/include/glm",
        "-L/opt/homebrew/Cellar/glm/1.0.1/lib",
        "-lGLM",
        "-framework",
        "OpenGL",
        "-O2"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
